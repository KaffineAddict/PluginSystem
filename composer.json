{
    "name": "KaffineAddict/PluginSystem",
    "description": "A CakePHP hooks component for basic user extensions",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "PluginSystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PluginSystem\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
